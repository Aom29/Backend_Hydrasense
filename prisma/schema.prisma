generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}
  
// =======================
// Modelo Account
// =======================
model Account {
  account_id Int      @id @default(autoincrement())
  email      String   @unique                     
  password   String                               
  role       String   @default("USER_ROLE") 
  email_validated Boolean @default(false)     
  created_at DateTime @default(now())            
}